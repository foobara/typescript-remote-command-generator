// imports needed for context types...
import <%= error_base_class %> from "<%= path_to_root %>base/Error"
<% binding.pry  %>
<% uniq_error_generators_by_symbol.each do |entity| %>
import {<%= entity.entity_name %>} from "<%= path_to_root %><%= entity.import_path %>"
<% end %>

interface <%= error_name %><
  keyT extends string,
  pathT extends string[] = [],
  runtime_pathT extends string[] = []
> extends <%= error_base_class %> {
  key: keyT
  symbol: "<%= symbol %>"
  path: pathT
  runtime_path: runtime_pathT
  context: <%= context_ts_type %>
}
